APP_NAME=Gateapp
APP_ENV=production
APP_KEY=base64:GENERATE_NEW_KEY_WITH_php_artisan_key:generate
APP_URL=https://yourdomain.com
APP_DEBUG=false
APP_LOG_LEVEL=error
APP_DEBUG_EMAILS=null
APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_ADDITIONAL_LOCALES=null
APP_TIMEZONE=UTC
APP_LOCALES=en

# DATABASE (Use production credentials)
DB_CONNECTION=mysql
DB_HOST=your-production-db-host
DB_PORT=3306
DB_DATABASE=gateapp_production
DB_USERNAME=gateapp_user
DB_PASSWORD=STRONG_PASSWORD_HERE

# QUEUE (Enable for production)
USE_QUEUE=1
QUEUE_CONNECTION=redis

# REDIS (For caching and queues)
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

# MAIL (Production mail service)
MAIL_DRIVER=mailgun
MAIL_HOST=smtp.mailgun.org
MAIL_PORT=587
MAIL_USERNAME=
MAIL_PASSWORD=
MAIL_ENCRYPTION=tls
MAIL_FROM_ADDRESS=noreply@yourdomain.com
MAIL_FROM_NAME="${APP_NAME}"
MAILGUN_DOMAIN=mg.yourdomain.com
MAILGUN_SECRET=

# AWS SES (Alternative to Mailgun)
SES_KEY=
SES_SECRET=
SES_REGION=us-east-1

# PAYMENT GATEWAY (Production keys)
STRIPE_KEY=pk_live_YOUR_LIVE_KEY
STRIPE_SECRET=sk_live_YOUR_LIVE_SECRET
PAYSTACK_SECURE_KEY=
MOLLIE_API_KEY=live_YOUR_LIVE_KEY
WIPAY_DEVELOPER_ID=
WIPAY_URL=https://tt.wipayfinancial.com
WIPAY_ACCOUNT_NUMBER=
WIPAY_VOUCHER_URL=https://tt.wipayfinancial.com/v1/voucher_pay
APPLE_INAPP_SHARED_SECRET=

# FIREBASE (Production credentials)
FIREBASE_ID=
FIREBASE_USE_NEW_RTDB_URI=1
FIREBASE_SERVICE_ACCOUNT_JSON=firebase-production.json

# ONESIGNAL (Production app IDs)
ONESIGNAL_APP_ID_CUSTOMER=
ONESIGNAL_REST_API_CUSTOMER=
ONESIGNAL_APP_ID_PROVIDER=
ONESIGNAL_REST_API_PROVIDER=

# MISC.
DEMO_ENABLED=false
MOBILE_NUMBER_LENGTH=10

# SOCIAL LOGIN (Production credentials)
GOOGLE_CLIENT_ID=
GOOGLE_IOS_CLIENT_ID=
GOOGLE_IOS_PROVIDER_CLIENT_ID=
FACEBOOK_APP_ID=
FACEBOOK_APP_SECRET=

# TELESCOPE (MUST be disabled in production)
TELESCOPE_ENABLED=false
TELESCOPE_CACHE_WATCHER=false
TELESCOPE_DUMP_WATCHER=false
TELESCOPE_JOB_WATCHER=false
TELESCOPE_MAIL_WATCHER=false
TELESCOPE_MODEL_WATCHER=false
TELESCOPE_REDIS_WATCHER=false
TELESCOPE_GATE_WATCHER=false
TELESCOPE_QUERY_WATCHER=false
TELESCOPE_NOTIFICATION_WATCHER=false
TELESCOPE_VIEW_WATCHER=false
TELESCOPE_SCHEDULE_WATCHER=false
TELESCOPE_COMMAND_WATCHER=false

# SESSION SECURITY
SESSION_DRIVER=redis
SESSION_LIFETIME=120
SESSION_SECURE_COOKIE=true
SESSION_HTTP_ONLY=true
SESSION_SAME_SITE=strict

# CACHE
CACHE_DRIVER=redis
CACHE_PREFIX=gateapp_cache

# LOGGING
LOG_CHANNEL=stack
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=error

# SECURITY
FORCE_HTTPS=true
